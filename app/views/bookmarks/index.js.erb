if (<%= @delete %>) {
  if (!<%= @itineraries.size.positive? %>) {
    element = document.querySelector('#bookmark a');
    element.innerHTML = '<i class="far fa-star"></i>';
  }
  itinerary = document.querySelector('#itinerary-id-<%= @itinerary.id %>');
  itinerary.remove();
}
